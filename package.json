{
  "name": "objection-authorize",
  "version": "0.6.2",
  "description": "\"magical\" access control integrated with objection.js",
  "main": "index.js",
  "repository": "JaneJeon/objection-authorize",
  "author": "Jane Jeon <JaneJeon9719@gmail.com>",
  "license": "MIT",
  "scripts": {
    "test": "jest",
    "test:watch": "yarn test --watch",
    "coverage": "is-ci && codecov",
    "lint": "standard | snazzy && prettier --check '**/*.{md,json,yml,yaml}'"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "pre-push": "yarn test"
    }
  },
  "lint-staged": {
    "*.js": [
      "prettier --write",
      "standard --fix",
      "git add"
    ],
    "*.{md,json,yml,yaml}": [
      "prettier --write",
      "git add"
    ]
  },
  "standard": {
    "env": [
      "jest"
    ]
  },
  "prettier": {
    "semi": false,
    "singleQuote": true
  },
  "dependencies": {
    "http-assert": "^1.4.1"
  },
  "peerDependencies": {
    "objection": "1.X"
  },
  "devDependencies": {
    "@types/jest": "^24.0.18",
    "codecov": "^3.6.1",
    "husky": "^3.0.7",
    "is-ci": "^2.0.0",
    "jest": "^24.9.0",
    "jest-junit": "^8.0.0",
    "knex": "^0.19.5",
    "lint-staged": "^9.4.0",
    "objection": "^1.6.11",
    "objection-visibility": "^0.4.0",
    "prettier": "^1.18.2",
    "role-acl": "^3.3.7",
    "snazzy": "^8.0.0",
    "sqlite3": "^4.1.0",
    "standard": "^14.3.1"
  },
  "keywords": [
    "objection",
    "objection.js",
    "knex",
    "plugin",
    "authorization",
    "auth",
    "authz",
    "accesscontrol",
    "access-control",
    "role-acl",
    "access",
    "control",
    "list",
    "rbac",
    "abac",
    "role",
    "roles",
    "permission",
    "attribute",
    "attributes",
    "acl",
    "security",
    "resource",
    "express"
  ]
}
